                                    <xs:documentation>Kurzbeschreibung</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element ref="description" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

