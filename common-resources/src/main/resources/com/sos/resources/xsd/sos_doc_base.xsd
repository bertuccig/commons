        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="codeexample" minOccurs="0" />
                <xs:element ref="indexterm"  maxOccurs="unbounded"/>
                <xs:element ref="list"  maxOccurs="unbounded"/>
