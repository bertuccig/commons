                <xs:element ref="Oracle" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="sosdocpdflink" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="indexterm" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="term" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="glossarlink" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="path" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="folder" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="doculink" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="list" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="codeexample" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="menu" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="formlabel" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="formvalue" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="key" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="button" minOccurs="0" maxOccurs="unbounded" />
                <!--
                    <xs:any namespace="##any" minOccurs="0" maxOccurs="unbounded" processContents="lax" />
                -->

                <xs:element name="ul" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="li" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                      <xs:element ref="p" minOccurs="0" maxOccurs="unbounded" />
                                    </xs:sequence>
                                    <xs:attribute name="bullet" type="xs:string" use="optional" />
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="em" type="pType" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="strong" type="pType" minOccurs="0" maxOccurs="unbounded" />
                <xs:element name="dl" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="dt" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="p" maxOccurs="unbounded" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="dd" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
