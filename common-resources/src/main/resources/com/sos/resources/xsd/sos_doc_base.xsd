    <xs:complexType name="pType" mixed="true">
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:choice>
                            <xs:any namespace="http://www.w3.org/2001/XInclude" processContents="lax" minOccurs="0" maxOccurs="unbounded" />

                <xs:element ref="sosdownload" />
                <xs:element ref="apilink" />
                <xs:element ref="sosdocsoshelplink" />
                <xs:element ref="p" />
                <xs:element ref="JADE" />
                <xs:element ref="SOSFTP" />
                <xs:element ref="JOC" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="JOE" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="joe" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="Unix" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="Windows" minOccurs="0" maxOccurs="unbounded" />

                <xs:element ref="envvar" />
                <xs:element ref="command" />
                <xs:element ref="file" />
                <xs:element ref="formfield" />
                <xs:element ref="jobname" />
                <xs:element ref="jobdoc" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wikilink" minOccurs="0" maxOccurs="unbounded" />
