                            numbered list, see html ol tag
                            </xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="unordered" >
                        <xs:annotation>
                            <xs:documentation>
                            bulleted list, see html ul tag
                            </xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="numbered" />
                    <xs:enumeration value="features" />
                    <xs:enumeration value="restrictions" />
                    <xs:enumeration value="limitations" />
                    <xs:enumeration value="tasks" />
                    <xs:enumeration value="prerequisites" />
                    <xs:enumeration value="def" >
